<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <navigation-rule>
        <from-view-id>/FirstTam.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/header.xhtml</to-view-id>
        <redirect>     
        </redirect>
        </navigation-case>
 </navigation-rule>
  <navigation-rule>
    <from-view-id>/public/*</from-view-id>
    <navigation-case>
      <from-outcome>logout</from-outcome>
      <to-view-id>/index.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
    <managed-bean>
        <managed-bean-name>registrationAll</managed-bean-name>
        <managed-bean-class>registrationAll.Bean.RegistrationAll</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>viewDoctorSchedule</managed-bean-name>
        <managed-bean-class>viewDoctorSchedule.Bean.ViewDoctorSchedule</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>prescription4D</managed-bean-name>
        <managed-bean-class>showmedicine_forpatient.Bean.Prescription4D</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>departmentEntry</managed-bean-name>
        <managed-bean-class>department.Bean.DepartmentEntry</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>showUsers</managed-bean-name>
        <managed-bean-class>registrationAll.Bean.ShowUsersInfo</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>usserSShow</managed-bean-name>
        <managed-bean-class>UsserSShow</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>viewDoctoeInfo</managed-bean-name>
        <managed-bean-class>doctorInfo.Bean.ViewDoctoeInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>